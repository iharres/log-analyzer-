package logAnalyzer;
import java.io.File;
import java.util.Vector;

public class logAnalyzer {

	 public static void main(String[] args) {
		 RecursiveLogTraversal logTraversal = new RecursiveLogTraversal(new File("."));
		 Vector<File> logFiles = new Vector<>();
		 logTraversal.traverse(new File("."), logFiles);
		 Vector<logEntry> Entry = new Vector<>();
		 for (File f : logFiles)
			 logTraversal.parseFile(f, Entry);
		   	Vector<Suspicious> suspicious = logTraversal.aggregate(Entry);

	    }

}
 